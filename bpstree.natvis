<?xml version="1.0" encoding="utf-8"?> 
<AutoVisualizer xmlns="http://schemas.microsoft.com/vstudio/debugger/natvis/2010">
  <Type Name="b_plus_size_tree&lt;*&gt;">
    <DisplayString>{{ size={root_.parent-&gt;size} }}</DisplayString>
    <Expand>
      <Item Name="[comparator]" ExcludeView="simple">(key_compare &amp;)root_</Item>
      <Item Name="[allocator]" ExcludeView="simple">(node_allocator_t &amp;)root_</Item>
    </Expand>
  </Type>
  <Type Name="b_plus_size_tree&lt;*&gt;::key_stack_t">
    <DisplayString Condition="true">{*(key_type *)buffer}</DisplayString>
  </Type>
  <Type Name="b_plus_size_tree&lt;*&gt;::node_t">
    <DisplayString Condition="level != 0">{{ inner:size={size},used={((inner_node_t *)this)-&gt;used} }}</DisplayString>
    <DisplayString Condition="level == 0">{{ leaf:size={size} }}</DisplayString>
  </Type>
</AutoVisualizer>